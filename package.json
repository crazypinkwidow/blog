{
  "name": "blog",
  "version": "0.0.1",
  "description": "my blog",
  "private": true,
  "scripts": {
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass _sass/main.scss assets/css/main.css",
    "css-postcss": "postcss --use autoprefixer --use cssnano --no-map --output assets/css/main.css assets/css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "deploy": "npm run css-deploy && npm run js-build",
    "js-build": "babel _javascript --out-dir lib",
    "js-watch": "npm run js-build -- --watch",
    "watch": "npm run css-watch | npm run js-watch",
    "build": "npm run css-deploy && node build",
    "start": "node server",
    "test": "echo \"Error: no test specified\" && exit 0",
    "pub": "git subtree push --prefix=public/ blog master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazypinkwidow/blog.git"
  },
  "keywords": [
    "Crazy",
    "Pink",
    "Widow"
  ],
  "author": "Aoi Eba",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/crazypinkwidow/blog/issues"
  },
  "homepage": "https://github.com/crazypinkwidow/blog#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "bootstrap": "^4.1.1",
    "jstransformer-handlebars": "^1.1.0",
    "metalsmith": "^2.3.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-inline-source": "^1.0.6",
    "metalsmith-layouts": "^2.1.0",
    "metalsmith-markdown": "^0.2.2",
    "metalsmith-permalinks": "^0.5.0"
  }
}
